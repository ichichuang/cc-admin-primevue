{
  "name": "cc-admin-primevue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "author": "",
  "description": "cc-admin-primevue 企业级后台管理框架 - 基于 Vue 3.5+、TypeScript 5+ 和 PrimeVue 的现代化企业级后台管理框架",
  "keywords": [
    "vue3",
    "typescript",
    "vite",
    "primevue",
    "pinia",
    "alova",
    "admin",
    "framework",
    "cc-admin-primevue"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "pnpm exec tsx scripts/dev-parallel.ts",
    "build": "vue-tsc --noEmit && vite build",
    "build:analyze": "vue-tsc --noEmit && vite build --mode analyze",
    "preview": "vite preview",
    "report": "npx vite-bundle-analyzer dist/stats.html",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "naming-check": "pnpm exec tsx scripts/naming-rules.ts",
    "naming-watch": "pnpm exec tsx scripts/watch-naming.ts",
    "check": "pnpm exec tsx scripts/check.ts",
    "env-check": "pnpm exec tsx scripts/check-env.ts",
    "watch:components": "pnpm exec tsx scripts/watch-components.ts",
    "component-watcher": "pnpm exec tsx scripts/component-watcher.ts",
    "update:components": "pnpm exec tsx scripts/update-components.ts",
    "fix": "pnpm format && pnpm lint",
    "pre-commit": "pnpm check",
    "commit": "git-cz",
    "prepare": "husky",
    "init": "pnpm exec tsx scripts/init.ts",
    "init:cc": "pnpm exec tsx scripts/init.ts"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less,styl,json,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pnpm": {
    "overrides": {
      "glob": "^11.0.3"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild"
    ]
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "^1.7.4",
    "@primevue/forms": "^4.3.7",
    "@primevue/themes": "^4.3.7",
    "@unocss/preset-rem-to-px": "^66.3.3",
    "alova": "^3.3.3",
    "animate.css": "^4.1.1",
    "crypto-js": "^4.2.0",
    "custom-vue-scrollbar": "^0.0.8",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.7",
    "quill": "^2.0.3",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.8",
    "vue-router": "^4.4.5",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@faker-js/faker": "^9.9.0",
    "@iconify/json": "^2.2.353",
    "@prettier/plugin-pug": "^3.4.0",
    "@primevue/auto-import-resolver": "^4.3.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@types/nprogress": "^0.2.3",
    "@types/postcss-pxtorem": "^6.1.0",
    "@types/pug": "^2.0.10",
    "@unocss/extractor-pug": "^0.61.9",
    "@unocss/preset-attributify": "^66.3.2",
    "@unocss/preset-icons": "^66.3.2",
    "@unocss/preset-typography": "^66.3.2",
    "@unocss/preset-uno": "^66.3.2",
    "@unocss/reset": "^66.3.2",
    "@unocss/transformer-directives": "^66.3.2",
    "@unocss/transformer-variant-group": "^66.3.2",
    "@vitejs/plugin-legacy": "^7.0.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/language-plugin-pug": "^3.0.5",
    "@vue/tsconfig": "^0.7.0",
    "chokidar": "^4.0.3",
    "commitizen": "^4.3.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.31.0",
    "html2pug": "^4.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "npm-run-all2": "^7.0.2",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.4.2",
    "pug": "^3.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass": "^1.89.2",
    "terser": "^5.36.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.18.1",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^20.0.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-eslint-parser-template-tokenizer-pug": "^1.0.0",
    "vue-tsc": "^2.2.10"
  }
}
